{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "module": "commonjs",
    "sourceMap": false,
    "noImplicitAny": false,
    "outDir": "build",
    "lib": [
      "esnext",
    ]
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts"
  ]
}
